#!/bin/bash
# read file and create INSERT statements for MYSQL
outfile='members.sql'
IFS=','
while read lname fname address city state zip
do
	cat >> $outfile <<EOF
	INSERT INTO members (lname,fname,address,city,state.zip) VALUES
	('$lname','$fname','$address','$city','$tate','$zip');
EOF
done <${1}
